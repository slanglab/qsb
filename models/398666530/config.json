{
    "dataset_reader": {
        "type": "binary_reader"
    },
    "iterator": {
        "type": "bucket",
        "batch_size": 64,
        "sorting_keys": [
            [
                "sentence",
                "num_tokens"
            ]
        ]
    },
    "model": {
        "type": "split_classifier",
        "abstract_encoder": {
            "type": "lstm",
            "bidirectional": true,
            "dropout": 0.025670178015253453,
            "hidden_size": 596,
            "input_size": 100,
            "num_layers": 1
        },
        "classifier_feedforward_i": {
            "activations": [
                "sigmoid"
            ],
            "dropout": 0.47406985591567374,
            "hidden_dims": [
                2
            ],
            "input_dim": 1192,
            "num_layers": 1
        },
        "classifier_feedforward_p": {
            "activations": [
                "sigmoid"
            ],
            "dropout": 0.47406985591567374,
            "hidden_dims": [
                2
            ],
            "input_dim": 1192,
            "num_layers": 1
        },
        "text_field_embedder": {
            "tokens": {
                "type": "embedding",
                "embedding_dim": 100,
                "pretrained_file": "https://s3-us-west-2.amazonaws.com/allennlp/datasets/glove/glove.6B.100d.txt.gz",
                "trainable": false
            }
        }
    },
    "train_data_path": "/mnt/nfs/work1/brenocon/ahandler/qsr/lstm_train_3way.jsonl",
    "validation_data_path": "/mnt/nfs/work1/brenocon/ahandler/qsr/lstm_validation_3way.jsonl",
    "trainer": {
        "cuda_device": 0,
        "grad_clipping": 5,
        "num_epochs": 3,
        "optimizer": {
            "type": "adam",
            "lr": 0.001873627420156746,
            "weight_decay": 0.00045421988121447727
        },
        "patience": 10,
        "validation_metric": "+accuracy"
    }
}